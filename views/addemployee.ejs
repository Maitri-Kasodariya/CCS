    <h1>Add Menu Item</h1>
    &nbsp;&nbsp;&nbsp;
    <form action="/showemployee/<%= canteenId %>/">
        <button type="submit" class="bt">Show Employee</button>
    </form>
    
    <form method="POST" action="/employees/<%= canteenId %>/employee/add">
        <label for="name">Name:</label>
        <input type="text" id="name" name="name"><br><br>

        <label for="Salary">Salary:</label>
        <input type="number" id="Salary" name="salary" ><br><br>

        <label for="phoneNumber">Contact Number:</label>
        <input type="number" id="phoneNumber" name="phoneNumber" ><br><br>

        <label for="work">Work:</label>
        <input type="text" id="work" name="work" ><br><br>

        <label for="address">Address:</label>
        <input type="text" id="address" name="address" ><br><br>

        <button type="submit">Add Employee</button>
    
    
        
    </form>
   
</body>
</html>
